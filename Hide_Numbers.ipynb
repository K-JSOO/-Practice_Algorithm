    "def hide_numbers(a):\n",
    "    anser = []\n",
    "    for i in range(len(a)-4):\n",
    "        anser.append('*')  \n",
    "    anser=','.join(anser)\n",
    "    anser=anser.replace(',', '')+a[-4:]\n",
    "    return anser"
   ]
