   "source": [
    "def solution(n, m):\n",
    "    x=[]\n",
    "    y=[]\n",
    "    for i in range(1,m+n):\n",
    "      if n%i==0 and m%i==0:\n",
    "          x.append(i)\n",
    "          y.append(round((n*m)/i))\n",
    "    answer=[max(x),min(y)]\n",
    "    return answer"
   ]
  },
